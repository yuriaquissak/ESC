/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Nand(a=a, b=ns, out=c);
	Nand(a=sel, b=b, out=d);
	Nand(a=c, b=d, out=out);
	Not(in=sel, out=ns);
}